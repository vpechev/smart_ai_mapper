{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "full_name": {
        "type": "string"
      },
      "personal_info": {
        "type": "object",
        "properties": {
          "age": {
            "type": "integer"
          }
        },
        "required": ["age"]
      },
      "enrolled_courses": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "required": ["id", "full_name", "personal_info", "enrolled_courses"]
  }