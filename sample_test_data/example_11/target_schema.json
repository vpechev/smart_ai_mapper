{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "type": {
        "type": "string"
      },
      "manufacturer": {
        "type": "string"
      },
      "price": {
        "type": "number",
        "minimum": 0
      },
      "release_date": {
        "type": "string",
        "format": "date"
      }
    },
    "required": ["id", "name", "type", "manufacturer", "price", "release_date"]
  }