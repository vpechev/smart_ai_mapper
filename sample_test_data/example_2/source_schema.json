{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "full_name": {
      "type": "string"
    },
    "birthdate": {
      "type": "string",
      "format": "date"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "zip_code": {
      "type": "string"
    },
    "category_code": {
      "type": "string"
    }
  },
  "required": ["full_name", "birthdate", "address", "city", "state", "zip_code", "category_code"]
}
